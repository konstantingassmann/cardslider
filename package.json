{
  "name": "cardslider",
  "version": "1.2.2",
  "description": "A simple and lightweight jQuery card slider",
  "main": "dist/js/jquery.cardslider.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konstantingassmann/cardslider.git"
  },
  "scripts": {
    "js:dev": "webpack --progress  --env.NODE_ENV=development --config webpack.config.js",
    "js:build": "webpack  --env.NODE_ENV=production --config webpack.config.js",
    "sass:compile": "sass src/scss/cardslider.scss:dist/css/cardslider.css --style compressed --sourcemap=none",
    "sass:watch": "sass --watch src/scss/cardslider.scss:dist/css/cardslider.css"
  },
  "keywords": [
    "card",
    "cards",
    "slider",
    "jquery",
    "carousel"
  ],
  "author": "Konstantin Gassmann konstantin.gassmann@gmail.com",
  "license": "WTFPL",
  "dependencies": {
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
